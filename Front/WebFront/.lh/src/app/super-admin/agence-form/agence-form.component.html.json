            "content": "<h2 mat-dialog-title>{{ isEditMode ? 'Modifier Agence' : 'Nouvelle Agence' }}</h2>\r\n\r\n<mat-dialog-content>\r\n  <form [formGroup]=\"agenceForm\">\r\n    <mat-form-field appearance=\"outline\">\r\n      <mat-label>Nom</mat-label>\r\n      <input matInput formControlName=\"name\" required>\r\n      <mat-error *ngIf=\"agenceForm.get('name')?.hasError('required')\">Champ obligatoire</mat-error>\r\n    </mat-form-field>\r\n\r\n    <mat-form-field appearance=\"outline\">\r\n      <mat-label>Adresse</mat-label>\r\n      <input matInput formControlName=\"address\" required>\r\n    </mat-form-field>\r\n\r\n    <mat-form-field appearance=\"outline\">\r\n      <mat-label>Email</mat-label>\r\n      <input matInput formControlName=\"email\" type=\"email\" required>\r\n      <mat-error *ngIf=\"agenceForm.get('email')?.hasError('email')\">Email invalide</mat-error>\r\n    </mat-form-field>\r\n\r\n    <mat-form-field appearance=\"outline\">\r\n      <mat-label>Téléphone</mat-label>\r\n      <input matInput formControlName=\"phone\" required>\r\n    </mat-form-field>\r\n  </form>\r\n</mat-dialog-content>\r\n\r\n<mat-dialog-actions align=\"end\">\r\n  <button mat-button mat-dialog-close>Annuler</button>\r\n  <button mat-raised-button color=\"primary\" (click)=\"onSubmit()\" [disabled]=\"!agenceForm.valid\">\r\n    {{ isEditMode ? 'Modifier' : 'Ajouter' }}\r\n  </button>\r\n</mat-dialog-actions>"
